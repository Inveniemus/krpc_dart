import 'package:fixnum/fixnum.dart' show Int64;
{{#imports}}
import '{{{filename}}}';
{{/imports}}

dynamic getClass(String service, String className, Int64 reference) {
  switch(service) {
    {{#services}}
    case '{{{service_name}}}':
      return get{{{service_name}}}Class(className, reference);
    {{/services}}
    default:
      throw Exception('in getClass, could not find the service!');
  }
}

dynamic getEnum(String service, String enumName, int index) {
  switch(service) {
    {{#services}}
    case '{{{service_name}}}':
      return get{{{service_name}}}Enum(enumName, index);
    {{/services}}
    default:
      throw Exception('in getEnum, could not find the service!');
  }
}
